{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist",
				"buildCommand": "npm run build",
				"devCommand": "npm run dev",
				"installCommand": "npm install"
			}
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"headers": {
				"Cache-Control": "no-cache"
			},
			"dest": "/index.html"
		}
	]
}
